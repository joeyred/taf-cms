    "what-input": "^4.0.4",
    "xo": "^0.17.0",
    "yamljs": "^0.2.8",
    "yargs": "^5.0.0"
  },
  "xo": {
    "space": true,
    "globals": [
      "angular"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "key-spacing": [
        "error",
        {
          "align": "value"
        }
      ],
      "no-multi-spaces": [
        "error",
        {
          "exceptions": {
            "Property": true,
            "VariableDeclarator": true
          }
        }
      ]
    }
  }
}
